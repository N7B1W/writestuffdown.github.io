<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Write Stuff Down</title>
 <link href="http://writestuffdown.org/atom.xml" rel="self"/>
 <link href="http://writestuffdown.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://writestuffdown.org/</id>
 <author>
   <name>Snowcrash</name>
   <email>snowcrash@protonmail.ch</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://writestuffdown.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://writestuffdown.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
